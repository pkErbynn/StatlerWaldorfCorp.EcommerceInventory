{
  "spring": {
    "application": {
      "name": "inventory"
    }
  },
  "eureka": {
    "client": {
      "serviceUrl": "http://192.168.0.33:8080/eureka/",
      "shouldRegisterWithEureka": true,
      "shouldFetchRegistry": false,
      "validate_certificates": false
    },
    "instance": {
      "appname": "inventory",
          "nonSecurePort": 5001,
          "port": 5001,
          "instanceId": "localhost:inventory:5001"
    }
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
        "Default": "Debug",
        "System": "Debug",
        "Microsoft": "Debug",
        "Steeltoe": "Debug"
    }
  }
}
